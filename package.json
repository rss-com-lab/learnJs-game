{
  "name": "math-game-web",
  "version": "0.1.0",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "eslint-config-react-app": "^5.1.0",
    "netlify-cli": "^2.29.0",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "rc-progress": "^2.5.2",
    "react": "^16.12.0",
    "react-dev-utils": "^10.0.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "react-syntax-highlighter": "^12.2.1",
    "redux": "^4.0.5",
    "redux-form": "^8.2.6",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.75"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "npm run style-css",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "netlify deploy",
    "start-js": "node scripts/start.js --mode=development",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "node scripts/build.js",
    "build": "npm-run-all build-css build-js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint src/**/*.js",
    "prettier": "eslint src/**/*.js --fix",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "style-css": "csscomb --tty-mode --",
    "style-all-css": "csscomb src/",
    "process": "node src/data/script"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@storybook/addon-actions": "^5.3.1",
    "@storybook/addon-links": "^5.3.1",
    "@storybook/addon-viewport": "^5.3.1",
    "@storybook/addons": "^5.3.1",
    "@storybook/react": "^5.3.1",
    "autoprefixer": "9.7.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^9.1.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "3.0.0",
    "css-loader": "3.4.2",
    "csscomb": "^4.3.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "5.0.2",
    "front-matter": "^3.1.0",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "husky": "^4.0.7",
    "jest": "24.9.0",
    "jest-cli": "^24.9.0",
    "lint-staged": "^9.5.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "promise": "8.0.3",
    "raf": "3.4.1",
    "resolve": "1.14.2",
    "sass-loader": "^8.0.1",
    "style-loader": "1.1.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-dev-server": "3.10.1",
    "webpack-manifest-plugin": "2.2.0",
    "whatwg-fetch": "3.0.0"
  },
  "homepage": "https://simple-math-game.netlify.com/",
  "contributors": [
    {
      "name": "Dzianis Sheka",
      "email": "dzianis.sheka@gmail.com",
      "url": "https://github.com/davojta"
    },
    {
      "name": "Alesja",
      "url": "https://github.com/AlesiaGit"
    }
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
